.container {
    background-color: #FFF;
    min-height: 100%;
}

.header {
    width: 750rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    padding-top: 40rpx;
    padding-bottom: 40rpx;
    background: #3274e5;
}

.header .city {
    height: 80rpx;
    line-height: 80rpx;
    color: #FFF;
    font-size: .8rem;
}

.header .search {
    width: 600rpx;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 5rpx;
    background: #FFF;
    display: flex;
    flex-direction: row;
    justify-content: center;
}

.header .search view {
    color: #a6a6a6;
    font-size: .7rem;
    align-self: center;
}

.header .search icon {
    margin-right: 30rpx;
    margin-top: 18rpx;
}

.swiper-container {
    width: 750rpx;
    position: relative;
}

.swiper_box {
    width: 100%;
}

swiper-item image {
    width: 100%;
    display: inline-block;
    overflow: hidden;
}

.swiper-container .dots {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 20rpx;
    display: flex;
    justify-content: center;
}

.swiper-container .dots .dot {
    margin: 0 8rpx;
    width: 14rpx;
    height: 14rpx;
    background: #fff;
    border-radius: 50%;
    transition: all .6s;
    opacity: .5;
}

.swiper-container .dots .dot.active {
    width: 14rpx;
    opacity: 1;
}

.navlist {
    width: 750rpx;
    display: flex;
    justify-content: space-around;
    flex-direction: row;
    padding-top: 20rpx;
    flex-wrap: wrap;
}

.navlist .item {
    width: 180rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    font-size: .7rem;
    padding-bottom: 30rpx;
}

.navlist .item image {
    width: 120rpx;
    height: 120rpx;
}

.addjob {
    width: 750rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
}

.addjob>view {
    width: 48%;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 10rpx;
    color: #FFF;
    font-size: .8rem;
    text-align: center;
}

.addjob>view:nth-child(1) {
    background: #62c7b5;
}

.addjob>view:nth-child(2) {
    background: #75aac9;
}

.news {
    margin-top: 10rpx;
    width: 750rpx;
    background: #FFF;
    display: flex;
    flex-direction: column;
}

.news .newstitle {
    width: 750rpx;
    height: 80rpx;
    line-height: 80rpx;
    margin-left: 20rpx;
    font-size: .7rem;
    border-bottom: 1rpx solid #efefef;
    margin: 0 auto;
    display: flex;
    flex-direction: row;
    align-content: center;
}

.news .newstitle view:nth-child(1) {
    height: 40rpx;
    line-height: 40rpx;
    width: 6rpx;
    background: #3274e5;
    align-self: center;
    margin-left: 10rpx;
}

.news .newstitle view:nth-child(2) {
    height: 80rpx;
    line-height: 80rpx;
    width: 150rpx;
    padding-left: 15rpx;
}

.type-container {
    width: 100%;
    background-color: #fff;
    overflow: hidden;
}

.type-navbar {
    white-space: nowrap;
    background-color: #FFF;
    height: 210rpx;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.type-box {
    padding-top: 20rpx;
    width: 250rpx;
    box-sizing: content-box;
    font-size: 24rpx;
    text-align: center;
    display: inline-block;
    overflow: hidden;
}

.type-box image {
    width: 230rpx;
    height: 230rpx;
}

.type-navbar-item {
    border-bottom: 4rpx solid #fff;
}

.type-item-on {
    border-bottom: 4rpx solid #e64340;
}

.news .newslist {
    display: flex;
    flex-direction: row;
    width: 720rpx;
    margin: 0 auto;
    border-bottom: 1px solid #efefef;
    padding-top: 10rpx;
    padding-bottom: 10rpx;
}

.news .newslist .title {
    width: 100%;
    flex-direction: column;
    display: flex;
    justify-content: space-between;
}

.news .newslist .title .address {
    padding-left: 2%;
    text-align: left;
    font-size: .8rem;
    color: #646464;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    width: 100%;
}

.news .newslist .title .address view:nth-child(1) {
    padding-top: 20rpx;
    color: #646464;
    font-weight: bold;
}

.news .newslist .title .detail {
    padding-left: 2%;
    text-align: left;
    font-size: .8rem;
    color: #000;
    display: flex;
    flex-direction: row;
    width: 100%;
}

.news .newslist .title .detail .pic {
    width: 20%;
    display: flex;
    align-items: center;
}

.news .newslist .title .detail .pic image {
    width: 120rpx;
    height: 120rpx;
    align-self: center;
}

.news .newslist .title .detail .info {
    width: 75%;
}

.news .newslist .title .detail .info>view:nth-child(1) {
    padding-top: 10rpx;
    color: #646464;
    font-size: .7rem;
}

.news .newslist .title .detail .info>view:nth-child(2) {
    padding-top: 10rpx;
    padding-bottom: 10rpx;
    color: #646464;
    font-size: .7rem;
}

.news .newslist .title .detail .info>view:nth-child(3) {
    color: #ff4129;
    font-weight: bold;
    font-size: .8rem;
    padding-right: 10rpx;
}

.news .newslist .title .price {
    width: 28%;
}

.news .newslist .title .price .money {
    color: #d65408;
    font-weight: bold;
    font-size: .7rem;
    padding-right: 10rpx;
}

.jobname>text {
    color: #b7b7b7;
}

.jobmoney {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.jobmoney text:nth-child(2) {
    font-weight: 600;
}

.special {
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    flex-wrap: wrap;
}

.special .item {
    margin-right: 10rpx;
    border-radius: 5rpx;
    padding: 10rpx;
    margin-top: 10rpx;
}

.special>.item:nth-child(2n-1) {
    background: #e4fcdc;
    color: #76af58;
}

.special>.item:nth-child(2n) {
    background: #fef5da;
    color: #ea994e;
}

.special>.item:nth-child(4n-1) {
    background: #daf2fe;
    color: #30a2c8;
}

.oldlist {
    display: flex;
    flex-direction: row;
    width: 720rpx;
    margin: 0 auto;
    border-bottom: 1px solid #efefef;
    padding-top: 10rpx;
    padding-bottom: 10rpx;
}

.oldlist .pic {
    width: 35%;
    height: 220rpx;
    line-height: 220rpx;
    align-items: center;
    justify-content: center;
    display: flex;
}

.oldlist .pic image {
    width: 260rpx;
    height: 210rpx;
}

.oldlist .title {
    width: 65%;
    flex-direction: column;
    display: flex;
    justify-content: space-between;
    height: 200rpx;
}

.oldlist .title .address {
    padding-left: 5%;
    text-align: left;
    font-size: .8rem;
    color: #000;
}

.oldlist .title .housearea {
    padding-left: 5%;
    text-align: left;
    font-size: .7rem;
    padding-top: 5rpx;
    padding-bottom: 5rpx;
    color: #000;
}

.oldlist .title .address text {
    color: #1aac19;
}

.oldlist .title .perprice {
    padding-left: 5%;
    font-size: .7rem;
    color: #d65408;
    font-weight: bold;
}

.oldlist .title .price {
    padding-left: 5%;
    text-align: left;
    font-size: .6rem;
    color: #6a6a6a;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    height: 60rpx;
    line-height: 60rpx;
    align-self: baseline;
    width: 450rpx;
}

.oldlist .title .price view text {
    padding: 5rpx 7rpx;
    border-radius: 3rpx;
    margin-right: 5rpx;
    height: 20rpx;
    line-height: 20rpx;
    color: #77af6e;
    background: #f7f5f6;
}

.oldlist .title .price .money {
    color: #d65408;
    font-weight: bold;
    font-size: .8rem;
    padding-right: 10rpx;
}

.case {
    margin-top: 10rpx;
    width: 750rpx;
    background: #FFF;
    display: flex;
    flex-direction: column;
}

.case .casetitle {
    width: 750rpx;
    height: 100rpx;
    line-height: 100rpx;
    font-size: .8rem;
    border-bottom: 1rpx solid #efefef;
    margin: 0 auto;
    display: flex;
    flex-direction: column;
    align-content: center;
    color: #e64340;
    font-weight: 500;
}

.case .casetitle view:nth-child(1) {
    height: 50rpx;
    line-height: 50rpx;
    width: 750rpx;
    align-self: center;
    text-align: center;
}

.case .casetitle view:nth-child(2) {
    height: 50rpx;
    line-height: 50rpx;
    width: 750rpx;
    align-self: center;
    text-align: center;
}

.type-container2 {
    width: 100%;
    height: 88rpx;
    background-color: #fff;
